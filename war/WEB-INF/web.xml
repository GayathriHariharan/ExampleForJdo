<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.example.exampleforjdo.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>com.example.exampleforjdo.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddMarkServlet</servlet-name>
		<servlet-class>com.example.exampleforjdo.AddMarkServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddMarkServlet</servlet-name>
		<url-pattern>/addmark</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ViewMarkServlet</servlet-name>
		<servlet-class>com.example.exampleforjdo.ViewMarkServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewMarkServlet</servlet-name>
		<url-pattern>/viewmark</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteMarkServlet</servlet-name>
		<servlet-class>com.example.exampleforjdo.DeleteMarkServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteMarkServlet</servlet-name>
		<url-pattern>/deletemark</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateMarkServlet</servlet-name>
		<servlet-class>com.example.exampleforjdo.UpdateMarkServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateMarkServlet</servlet-name>
		<url-pattern>/updatemark</url-pattern>
	</servlet-mapping>
		<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
